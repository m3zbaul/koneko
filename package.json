{
  "name": "react-boilerplate",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "axios": "^0.18.1",
    "node-sass": "^4.12.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-redux": "^7.0.2",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.0",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "reselect": "^4.0.0"
  },
  "scripts": {
    "start": "cross-env BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --passWithNoTests",
    "eject": "react-scripts eject",
    "precommit": "lint-staged"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "cross-env": "^5.2.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "lint-staged": "^9.2.5",
    "prettier": "1.18.2"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint src --ext .js",
      "prettier --write",
      "git add"
    ]
  }
}
